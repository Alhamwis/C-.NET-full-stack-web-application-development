                            <li class="nav-item" style="display:block">
                                <a class="nav-link" href="https://localhost:44323/Adherent/Index">HumainCO</a>
                            </li>
                        }
                        <li style="padding = 20px !important; display: block; margin-left: 15px;">@Html.ActionLink("Home", "Index", "Home", new { @class = "nav-link" })</li>

                        <li style="padding = 20px !important; display: block; margin-left: 15px;">@Html.ActionLink("About ", "About", "Home", new { @class = "nav-link" })</li>
                        @if (Session["qui"] == "adherent")
                        {
                            <li class="nav-item" style="display:none">
                                <a class="nav-link" href="https://localhost:44323/inscription.aspx">Contact</a>
                            </li>
                            <li class="nav-item" style="display:block">
                                <a class="nav-link" href="https://localhost:44323/MesInfos.aspx?Email=@Session["adhd"]">My Profile</a>
                            </li>
                            <li class="nav-item" style="display:block">
                                <a class="nav-link" href="https://localhost:44323/authentification.aspx">Se Deconnecter</a>
                            </li>
                        }


                        @*<li style="padding = 20px !important; display: block;">@Html.ActionLink("Services", "Services", "Home", new { @class = "nav-link" })</li>*@
                    </ul>
                </div>
                <div class="d-lg-block d-none">
                    <a href="#" class="btn btn-style btn-secondary ml-lg-3">Free Quote</a>
                </div>
                <!-- toggle switch for light and dark theme -->
                <div class="mobile-position">
                    <nav class="navigation">
                        <div class="theme-switch-wrapper">
                            <label class="theme-switch" for="checkbox">
                                <input type="checkbox" id="checkbox">
                                <div class="mode-container">
                                    <i class="gg-sun"></i>
                                    <i class="gg-moon"></i>
                                </div>
                            </label>
                        </div>
                    </nav>
                </div>
                <!-- //toggle switch for light and dark theme -->
            </nav>
        </div>
    </header>
    <!-- //header -->
